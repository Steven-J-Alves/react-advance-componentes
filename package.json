{
  "name": "react-advance-componentes",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:Steven-J-Alves/react-advance-componentes.git",
  "author": "Steven Alves <steven.alves@advectus.net>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --mode development",
    "build-debug": "cross-env NODE_ENV=test webpack --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "tailwind-build": "cross-env NODE_ENV=production postcss src/styles/tailwind.css -o dist/style/tailwind/tailwindBuild.css",
    "tailwind-watch": "postcss src/styles/tailwind.css -o dist/style/tailwind/tailwindBuild.css --watch",
    "scss": "node-sass --watch src/styles/scss/mainScss.scss -o dist/style/scss-compiled",
    "server": "json-server --watch statuses.json --port 5001"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-react": "^7.17.12",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.16.0",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.6.2",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.24",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  }
}
